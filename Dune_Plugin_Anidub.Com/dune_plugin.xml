<dune_plugin>
  <name>anidub</name>
  <caption>online.anidub</caption>
  <entry_points>
        <entry_point>
            <parent_media_url>root://video</parent_media_url>
            <media_url>main_menu</media_url>
            <caption>online.anidub</caption>
            <icon_url>plugin_file://skins/logo.png</icon_url>
            <actions>
                <key_enter>
                    <type>plugin_handle_user_input</type>
                </key_enter>
            </actions>
            <ip_address_required>yes</ip_address_required>
            <show_cookie_name>main_icon</show_cookie_name>
            <show_by_default>yes</show_by_default>
        </entry_point>
        <entry_point>
            <parent_media_url>setup://applications</parent_media_url>
            <media_url>setup</media_url>
            <caption>online.anidub</caption>
            <icon_url>plugin_file://skins/logo.png</icon_url>
            <actions>
                <key_enter>
                    <type>plugin_handle_user_input</type>
                </key_enter>
            </actions>
        </entry_point>
  </entry_points>
  <type>php</type>
  <params>
     <program>anidub.php</program>
  </params>
    <version_index>2</version_index>
  <version>1.0.1</version>
  <check_update>
    <schema>1</schema>
    <url>http://dl.dropboxusercontent.com/u/41185196/Dune/Update/anidub/update.txt</url>
    <timeout>0</timeout>
  </check_update>
</dune_plugin>